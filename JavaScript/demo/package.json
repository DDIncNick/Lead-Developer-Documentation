{
  "name": "project",
  "version": "1.0.0",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "build": "cross-env ENV=production webpack --config webpack.config.mjs",
    "dev": "cross-env ENV=dev webpack serve --config webpack.config.mjs --hot"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "arrow-spacing": [
        "warn",
        {
          "before": true,
          "after": true
        }
      ],
      "brace-style": [
        "error",
        "allman",
        {
          "allowSingleLine": true
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": [
        "error",
        {
          "after": true
        }
      ],
      "curly": [
        "error",
        "multi",
        "consistent"
      ],
      "dot-location": [
        "error",
        "property"
      ],
      "handle-callback-err": "off",
      "indent": [
        "warn",
        "tab"
      ],
      "max-nested-callbacks": [
        "error",
        {
          "max": 4
        }
      ],
      "max-statements-per-line": [
        "error",
        {
          "max": 2
        }
      ],
      "no-console": "off",
      "no-empty-function": "error",
      "no-floating-decimal": "error",
      "no-shadow": [
        "error",
        {
          "allow": [
            "err",
            "resolve",
            "reject"
          ]
        }
      ],
      "no-unused-vars": 0,
      "no-var": "warn",
      "no-lonely-if": "off",
      "no-undef": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-const": "warn",
      "quotes": [
        "error",
        "single"
      ],
      "semi": "error",
      "no-trailing-spaces": "off",
      "space-before-blocks": [
        "error",
        "never"
      ],
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "space-in-parens": [
        "error",
        "never"
      ],
      "yoda": "warn"
    }
  },
  "keywords": [],
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.17.0",
    "fs-extra": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "underscore": "^1.13.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  },
  "description": "",
  "dependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "jsdom": "^19.0.0"
  }
}
